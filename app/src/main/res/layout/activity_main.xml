<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/buttonView"
        android:layout_width="103dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="109dp"
        android:text="VIEW"
        android:textSize="14sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="131dp" />

    <Button
        android:id="@+id/buttonImaView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView2"
        android:layout_marginStart="12dp"
        android:layout_marginTop="138dp"
        android:layout_toEndOf="@+id/buttonView"
        android:text="IMAGEM VIEW"
        android:textSize="14sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="131dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="174dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="103dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="107dp"
        android:text="Exercicio Duplas"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="46dp" />

    <Button
        android:id="@+id/buttonWebView"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/buttonImaView"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-236dp"
        android:layout_marginEnd="261dp"
        android:layout_marginBottom="1dp"
        android:layout_toEndOf="@+id/buttonImaView"
        android:text="WEB VIEW"
        android:textStyle="bold" />

    <Button
        android:id="@+id/buttoncalendView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/buttonView"
        android:layout_alignParentTop="true"
        android:layout_marginTop="306dp"
        android:layout_marginEnd="-33dp"
        android:text="Calender View"
        android:textAllCaps="true"
        android:textStyle="bold" />

    <Button
        android:id="@+id/buttonProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="136dp"
        android:text="PROGESSBAR"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/buttonProgressBar"
        android:layout_marginStart="7dp"
        android:layout_marginBottom="0dp"
        android:layout_toEndOf="@+id/textView2"
        android:text="Button" />

    <Button
        android:id="@+id/buttonVideoView"
        android:layout_width="109dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/buttonView"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-4dp"
        android:layout_marginBottom="221dp"
        android:text="VideoView"
        android:textStyle="bold" />

    <Button
        android:id="@+id/buttonSeekBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/buttonVideoView"
        android:layout_marginStart="13dp"
        android:layout_marginBottom="0dp"
        android:layout_toEndOf="@+id/buttonVideoView"
        android:text="SEERKBAR"
        android:textStyle="bold" />

    <Button
        android:id="@+id/buttonRatingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/buttoncalendView"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="120dp"
        android:text="RATINGBAR"
        android:textStyle="bold" />

</RelativeLayout>